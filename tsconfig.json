{
    "compilerOptions": {
      "jsx": "react",
      "noImplicitAny": true,
      "noImplicitThis": true,
      "noUnusedLocals": true,
      "removeComments": true,
      "target": "ES5",
      "lib": ["es2015", "dom"]
    },
    "include": [
      "src/**/*.tsx",
      "src/**/*.ts",
      ".d.ts"
    ],
    "exclude": [
      "src/**/*.spec.*",
    ]
  }
  